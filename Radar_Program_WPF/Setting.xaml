            <ComboBoxItem>0 %</ComboBoxItem>
            <ComboBoxItem>25%</ComboBoxItem>
            <ComboBoxItem>50%</ComboBoxItem>
            <ComboBoxItem>75%</ComboBoxItem>
            <ComboBoxItem>90%</ComboBoxItem>
            <ComboBoxItem>99%</ComboBoxItem>
            <ComboBoxItem>99.9%</ComboBoxItem>
            <ComboBoxItem>100%</ComboBoxItem>
        </ComboBox>
        <ComboBox x:Name="ProbExists_MAX_input" Height="19" Margin="667,512,36.714,0" VerticalAlignment="Top">
            <ComboBoxItem>0 %</ComboBoxItem>
            <ComboBoxItem>25%</ComboBoxItem>
            <ComboBoxItem>50%</ComboBoxItem>
            <ComboBoxItem>75%</ComboBoxItem>
            <ComboBoxItem>90%</ComboBoxItem>
            <ComboBoxItem>99%</ComboBoxItem>
            <ComboBoxItem>99.9%</ComboBoxItem>
            <ComboBoxItem>100%</ComboBoxItem>
        </ComboBox>
        <Label Content="Min" Margin="566,118,137.714,828"/>
        <Label Content="Max" Margin="667,118,36.714,828"/>
        <CheckBox x:Name="NofObj_Valid_ckb" Content="NofObj" HorizontalAlignment="Left" Margin="426,152,0,0" VerticalAlignment="Top" Height="25" Width="122" FontSize="15"/>
        <CheckBox x:Name="Distance_Valid_ckb" Content="Distance" HorizontalAlignment="Left" Margin="426,197,0,0" VerticalAlignment="Top" Height="25" Width="122" FontSize="15"/>
        <CheckBox x:Name="VrelOncome_Valid_ckb" Content="VrelOncome" HorizontalAlignment="Left" Margin="426,287,0,0" VerticalAlignment="Top" Height="25" Width="122" FontSize="15"/>
