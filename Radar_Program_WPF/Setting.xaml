            <TextBox x:Name="VXDepart_MIN_input" TextWrapping="Wrap" Margin="0,0,101,-0.143"/>
            <TextBox x:Name="VXDepart_MAX_input" TextWrapping="Wrap" Margin="101,0,0,-0.143"/>

        </Grid>

