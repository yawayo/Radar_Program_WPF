            <ComboBoxItem>no sorting</ComboBoxItem>
            <ComboBoxItem>Sorted by range</ComboBoxItem>
            <ComboBoxItem>Sorted by RCS</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="StoreInNVM_ckb" Content="StoreInNVM" Checked="StoreInNVM_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,640,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="StoreInNVM_input" HorizontalAlignment="Left" Height="19" Margin="250,640,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Inactive</ComboBoxItem>
            <ComboBoxItem>active</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="RCS_Threshold_ckb" Content="RCS Threshold" Checked="RCSThreshold_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,700,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="RCS_Threshold_input" HorizontalAlignment="Left" Height="19" Margin="250,700,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Standard</ComboBoxItem>
            <ComboBoxItem>High sensitivity</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="InvalidClusters_ckb" Content="InvalidClusters" Checked="InvalidClusters_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,760,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="InvalidClusters_input" HorizontalAlignment="Left" Height="19" Margin="250,760,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Disable invalid cluster</ComboBoxItem>
            <ComboBoxItem>Enable all invalid clusters</ComboBoxItem>
            <ComboBoxItem>Enable low RCS dynamic</ComboBoxItem>
            <ComboBoxItem>Enable low RCS static</ComboBoxItem>
            <ComboBoxItem>Enable invalid range rate</ComboBoxItem>
            <ComboBoxItem>Enable range 1m</ComboBoxItem>
            <ComboBoxItem>Enable ego mirror</ComboBoxItem>
            <ComboBoxItem>Enable wrqpped stationary</ComboBoxItem>
        </ComboBox>

