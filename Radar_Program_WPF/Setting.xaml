        </Grid>
        <Grid Margin="566,737,36.714,212" RenderTransformOrigin="0.456,2.789">
            <TextBox x:Name="VYLeftRight_MIN_input" TextWrapping="Wrap" Margin="0,0,101,-0.143"/>
            <TextBox x:Name="VYLeftRight_MAX_input" TextWrapping="Wrap" Margin="101,0,0,-0.143"/>

        </Grid>
