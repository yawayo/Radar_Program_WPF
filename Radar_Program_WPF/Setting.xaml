        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Radar_Program_WPF"
        mc:Ignorable="d"
        Title="Setting" Height="1000" Width="800">
    <Grid Height="971" Margin="0,0,0.286,0.429" VerticalAlignment="Bottom">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="800" Margin="17,60,0,0" VerticalAlignment="Top" Width="370">
            <Label Content="Radar Setting" FontSize="20" Margin="8.857,25.857,219.143,732.857"/>
        </Border>
        <Border BorderBrush="Black" BorderThickness="1" Margin="407,60,9.714,111">
            <Label Content="Fillter Setting" FontSize="20" Margin="8.857,25.857,219.143,732.857"/>
        </Border>
        <Button x:Name="Radar_Write_btn" Content="Button" HorizontalAlignment="Left" Height="45" Margin="280,885,0,0" VerticalAlignment="Top" Width="110" Click="Radar_Write_btn_Click"/>
        <Button x:Name="Filter_Write_btn" Content="Button" HorizontalAlignment="Left" Height="45" Margin="667,885,0,0" VerticalAlignment="Top" Width="110" Click="Filter_Write_btn_Click"/>

        <CheckBox x:Name="MaxDistance_Valid_ckb" Content="Max Distance(0~2046)" Checked="MaxDistance_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,160,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <TextBox x:Name="MaxDistance_input" HorizontalAlignment="Left" Height="22" Margin="250,161,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90"/>

        <CheckBox x:Name="SensorID_ckb" Content="SensorID" Checked="SensorID_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,220,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="SensorID_input" HorizontalAlignment="Left" Height="19" Margin="250,220,0,0" VerticalAlignment="Top" Width="90" >
            <ComboBoxItem>0</ComboBoxItem>
            <ComboBoxItem>1</ComboBoxItem>
            <ComboBoxItem>2</ComboBoxItem>
            <ComboBoxItem>3</ComboBoxItem>
            <ComboBoxItem>4</ComboBoxItem>
            <ComboBoxItem>5</ComboBoxItem>
            <ComboBoxItem>6</ComboBoxItem>
            <ComboBoxItem>7</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="OutputType_ckb" Content="OutputType" Checked="OutputType_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,280,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="OutputType_input" HorizontalAlignment="Left" Height="19" Margin="250,280,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>None</ComboBoxItem>
            <ComboBoxItem>send Object</ComboBoxItem>
            <ComboBoxItem>send Cluster</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="RadarPower_ckb" Content="RadarPower" Checked="RadarPower_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,340,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="RadarPower_input" HorizontalAlignment="Left" Height="19" Margin="250,340,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Standard</ComboBoxItem>
            <ComboBoxItem>-3dB Tx gain</ComboBoxItem>
            <ComboBoxItem>-6dB Tx gain</ComboBoxItem>
            <ComboBoxItem>-9dB Tx gain</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="CtrlRelay_ckb" Content="CtrlRelay" Checked="CtrlRelay_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,400,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="CtrlRelay_input" HorizontalAlignment="Left" Height="19" Margin="250,400,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Inactive</ComboBoxItem>
            <ComboBoxItem>active</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="SendQuality_ckb" Content="SendQuality" Checked="SendQuality_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,460,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="SendQuality_input" HorizontalAlignment="Left" Height="19" Margin="250,460,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Inactive</ComboBoxItem>
            <ComboBoxItem>active</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="SendExtInfo_ckb" Content="SendExtInfo" Checked="SendExtInfo_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,520,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
        <ComboBox x:Name="SendExtInfo_input" HorizontalAlignment="Left" Height="19" Margin="250,520,0,0" VerticalAlignment="Top" Width="90">
            <ComboBoxItem>Inactive</ComboBoxItem>
            <ComboBoxItem>active</ComboBoxItem>
        </ComboBox>

        <CheckBox x:Name="SortIndex_ckb" Content="SortIndex" Checked="SortIndex_Valid_ckb_Checked" HorizontalAlignment="Left" Margin="32,580,0,0" VerticalAlignment="Top" Height="25" Width="180" FontSize="15"/>
